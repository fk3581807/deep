{
  "builds": [
    {
      "src": "/server.js",
      "dest": "/",
      "ssr": true
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "/webhook",
      "type": "http"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "PORT": 3000,
    "TelegramBotToken": "<your-telegram-bot-token>"
  }
}
